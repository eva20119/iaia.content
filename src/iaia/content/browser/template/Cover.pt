<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    lang="en"
    metal:use-macro="context/main_template/macros/master"
    i18n:domain="plone">
<body>

<metal:content-core fill-slot="content-core">
<metal:content-core define-macro="content-core"
                    tal:define="toc context/table_of_contents|nothing;">


<metal:macro use-macro="context/@@iaia_macro/macros/slider" />
<metal:macro use-macro='context/@@iaia_macro/macros/mini_nav' />
<metal:macro use-macro='context/@@iaia_macro/macros/youtube' />
<metal:macro use-macro='context/@@iaia_macro/macros/countdown' />
<metal:macro use-macro='context/@@iaia_macro/macros/news' />
<metal:macro use-macro='context/@@iaia_macro/macros/scenery' />
<metal:macro use-macro='context/@@iaia_macro/macros/farmer' />


</metal:content-core>
</metal:content-core>

</body>
</html>
